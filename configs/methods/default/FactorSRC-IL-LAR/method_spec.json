{
  "model_spec": {
    "name": "FactorTrainer",
    "params": {
      "args": {
        "latent_dim": 64,
        "lambda_data_source": 10.0,
        "lambda_grl": 0.0,
        "lambda_xpred": 0.0,
        "isolated_latents": true,
        "bn_mode": "none",
        "association_type": "learn",
        "lambda_asso": 5,
        "lambda_asso_start_epoch": 5,
        "lambda_asso_end_epoch": 15,
        "asso_reg_type": "entropy",
        "lambda_asso_suppress": 20
      }
    }
  },
  "learning_rule": {
    "num_epochs": 100,
    "term_gap_epochs": 20,
    "product_start_epoch": 15,
    "lr": 0.001,
    "wd": 5e-05,
    "dataloader_spec": {
      "params": {
        "batch_size": 512,
        "num_workers": 8
      }
    }
  }
}
